<div class="container">
<div class="row">
<div class="col m6 offset-m3">
	<h1 class="center light-blue lighten-3 z-depth-1">Add a Review</h1>
</div>
<div class="col m10 offset-m1">
<%= form_for @rating do |form| %>
Rated User: <%= form.text_field :rated_user %></br>
Title: <%= form.text_field :title %></br>
Comment: <%= form.text_field :comment %></br>
Rating: <%= form.text_field :rating %></br>
<%= form.submit %>
<% end %>
</div>
</div>
</div>

